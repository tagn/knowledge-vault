{
  "command_timeout": 5,
  "templates_folder": "Templates",
  "templates_pairs": [
    [
      "get_next_file_number",
      "dirname \"<% tp.file.path() %>\" | ls | awk '{print $NF}' | awk -F '.' '{print $1}' | sort -rn | head -n 1"
    ],
    [
      "increment",
      "echo $(echo <% tp.file.path(relative=true) %> | awk -F '/' '{print $NF}')"
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": false,
  "enable_system_commands": true,
  "shell_path": "C:/Windows/system32/bash.exe",
  "user_scripts_folder": "Scripts",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "drakkenheim/Sessions",
      "template": "Templates/Session.md"
    },
    {
      "folder": "DND - Drakkenheim/Characters",
      "template": "Templates/Character.md"
    },
    {
      "folder": "DND - Drakkenheim/Locations",
      "template": "Templates/Location.md"
    }
  ],
  "syntax_highlighting": true,
  "enabled_templates_hotkeys": [
    ""
  ],
  "startup_templates": [
    ""
  ],
  "enable_ribbon_icon": true
}